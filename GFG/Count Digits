//{ Driver Code Starts
#include <bits/stdc++.h>
using namespace std;


// } Driver Code Ends
class Solution {
  public:
  int evenlyDivides(int n){
  int dup = n;
    int cnt = 0;
    while(dup > 0){
        int digit = dup % 10;
        if (digit != 0 && n % digit == 0)
                cnt++;
    dup /= 10;
    }
    return cnt;
    }
};

//{ Driver Code Starts.
int main() {
    int t;
    cin >> t;
    while (t--) {
        int N;
        cin >> N;
        Solution ob;
        cout << ob.evenlyDivides(N) << endl;

        cout << "~"
             << "\n";
    }
    return 0;
}
// } Driver Code Ends
