bool palindrome(int n)
{
    bool pal;
    int reverse_n=0;
    int dup = n;
    while(n != 0){
        int last_digit=n%10;
        n=n/10;
        reverse_n=(reverse_n*10)+last_digit;       
    }
        if(dup==reverse_n){
            pal = true;
        }else if(dup!=reverse_n){
            pal = false;
        }
    
    return pal;
}
